---
interface Props {
    title: string;
    data: string;
}

const { title } = Astro.props;
const { data } = Astro.props;
---

<div class="w-1/3">
    <div class="flex items-center p-1">
        <h4 class="font-semibold text-sm">{title}</h4>: <p class="ml-2" id={`${data}`}>
        </p>
    </div>
</div>
